<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel = "methods" name="Suite" thread-count="2">

    <listeners>
        <listener class-name="TestComponents.Listeners"/>
    </listeners>


    <test name="Wrong credentials">
        <classes>
            <class name="tests.WrongCredentialsTest"/>
        </classes>
    </test>

    <test name="Add items to cart">
        <classes>
            <class name="tests.AddItemsToCartTest"/>
        </classes>
    </test>

    <test name="Empty cart">
        <classes>
            <class name="tests.EmptyCartTest"/>
        </classes>
    </test>

</suite>
